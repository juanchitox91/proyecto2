using System.Collections.Generic;
using System.ComponentModel;
using System.ComponentModel.DataAnnotations;

namespace SGEA.Models
{
    public class Arancel
    {
        public long ID { get; set; }
        [DisplayName("Monto Inscripción")]
        public string MontoInscripcion { get; set; }
        [DisplayName("Matrícula Anual")]
        public string MatriculaAnual { get; set; }
        [DisplayName("Año Lectivo")]
        public string AnhoLectivo { get; set; }
        [DisplayName("Observación")]
        public string Observacion { get; set; }
        [DisplayName("Nombre Arancel")]
        public string NombreArancel { get; set; }
        [DisplayName("Institucion")]
        public long InstitucionId { get; set; }

    }

}